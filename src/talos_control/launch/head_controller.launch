<?xml version="1.0" encoding="UTF-8"?>

<launch>
      
    <!-- Load controllers -->
    <rosparam command="load" file="$(find talos_control)/config/head.yaml" />
    
    <!-- Controllers -->
    <node name="head_control_spawner" pkg="controller_manager" type="spawner"
        respawn="false" output="screen" ns="/talos"
        args="--timeout 120
        head_tilt_controller
        head_pan_controller">
    </node>
    
